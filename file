CREATE TABLE vertica_snowflake_view_data_comparison (
    id IDENTITY,
    view_name VARCHAR(255),
    column_name VARCHAR(255),
    value_in_vertica VARCHAR(255),
    value_in_snowflake VARCHAR(255),
    unique_key VARCHAR(255),
    insrt_ts TIMESTAMP DEFAULT (statement_timestamp()),
    updt_ts TIMESTAMP,
    etl_trns VARCHAR(50),
    etl_job VARCHAR(50)
);